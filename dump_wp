UPDATE wp_options SET option_value = replace(option_value, 'https://kineis-production.cegedim.cloud', 'https://www.kineis.com') WHERE option_name = 'home' OR option_name = 'siteurl';
UPDATE wp_posts SET post_content = replace(post_content, 'https://kineis-production.cegedim.cloud', 'https://www.kineis.com');
UPDATE wp_postmeta SET meta_value = replace(meta_value,'https://kineis-production.cegedim.cloud','https://www.kineis.com');
UPDATE wp_usermeta SET meta_value = replace(meta_value, 'https://kineis-production.cegedim.cloud','https://www.kineis.com');

UPDATE wp_links SET link_url = replace(link_url, 'https://kineis-production.cegedim.cloud','https://www.kineis.com');

UPDATE wp_comments SET comment_content = replace(comment_content , 'https://kineis-production.cegedim.cloud','https://www.kineis.com');
UPDATE wp_posts SET post_content = replace(post_content, 'https://kineis-production.cegedim.cloud', 'https://www.kineis.com');
UPDATE wp_links SET link_image = replace(link_image, 'https://kineis-production.cegedim.cloud','https://www.kineis.com');
UPDATE wp_posts SET guid = replace(guid, 'https://kineis-production.cegedim.cloud','https://www.kineis.com');
