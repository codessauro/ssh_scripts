SELECT DISTINCT CONCAT('SELECT * FROM `', TABLE_SCHEMA, '`.`', TABLE_NAME, '` WHERE `', COLUMN_NAME, '` LIKE ''%https://kineis-preprod.cegedim.cloud%'';') 
FROM INFORMATION_SCHEMA.COLUMNS 
WHERE TABLE_SCHEMA NOT IN ('information_schema', 'mysql', 'performance_schema', 'sys')
    AND COLUMN_TYPE LIKE '%char%';


SELECT CONCAT('UPDATE ', TABLE_SCHEMA, '.', TABLE_NAME, ' SET ', COLUMN_NAME, " = REPLACE(", COLUMN_NAME, ", 'https://kineis-preprod.cegedim.cloud', 'https://kineis-production.cegedim.cloud') ;") AS statement
FROM information_schema.COLUMNS
WHERE TABLE_SCHEMA = 'wp-kineis' AND COLUMN_NAME LIKE '%url%';


UPDATE `wp-kineis`.`wp_yoast_seo_links`
SET `region` = REPLACE(`region`, 'https://kineis-preprod.cegedim.cloud', 'https://kineis-production.cegedim.cloud')
WHERE `region` LIKE '%https://kineis-preprod.cegedim.cloud%';

# Changer l'URL du site
UPDATE wp_options
SET option_value = replace(option_value, 'https://kineis-preprod.cegedim.cloud','https://kineis-production.cegedim.cloud')
WHERE option_name = 'home'
OR option_name = 'siteurl';
# Changer l'URL des GUID
UPDATE wp_posts
SET guid = REPLACE (guid, 'https://kineis-preprod.cegedim.cloud','https://kineis-production.cegedim.cloud');
# Changer l'URL des médias dans les articles et pages
UPDATE wp_posts
SET post_content = REPLACE (post_content, 'https://kineis-preprod.cegedim.cloud','https://kineis-production.cegedim.cloud');
# Changer l'URL des données meta
UPDATE wp_postmeta
SET meta_value = REPLACE (meta_value, 'https://kineis-preprod.cegedim.cloud','https://kineis-production.cegedim.cloud');

